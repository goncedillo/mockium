{
  "name": "mockium",
  "version": "1.1.0",
  "description": "Versatile, scalable and adaptable mock server based on the simplicity of mocking API requests.",
  "main": "index.js",
  "bin": {
    "mockium": "./bin/mockium.js",
    "mockium_features": "./bin/mockium-features.js",
    "mockium_server": "./bin/mockium-server.js"
  },
  "scripts": {
    "preinstall": "if [ $NODE_ENV != \"production\" ] && [ $NODE_ENV != \"ci\" ] ; then npm i -g stmux ; fi",
    "test": "jest --coverage --no-cache",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "version": "auto-changelog -p --template keepachangelog && git add CHANGELOG.md"
  },
  "keywords": [
    "mock",
    "mock-server",
    "fake",
    "server",
    "server",
    "mock",
    "server",
    "api",
    "test"
  ],
  "author": "GonzaloCedillo <goncedillo@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "ascii-text-generator": "^1.0.5",
    "asciify-image": "^0.1.3",
    "auto-changelog": "^1.13.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "cors": "^2.8.5",
    "decache": "^4.5.1",
    "express": "^4.16.4",
    "inquirer": "^6.2.2",
    "morgan": "^1.9.1",
    "node-watch": "^0.6.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.6",
    "stmux": "^1.6.1",
    "url-matcher": "^0.2.2",
    "ws": "^6.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.4",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "rollup-plugin-multi-entry": "^2.1.0"
  },
  "homepage": "https://github.com/goncedillo/mockium",
  "repository": {
    "type": "git",
    "url": "https://github.com/goncedillo/mockium.git"
  },
  "bugs": {
    "email": "goncedillo@gmail.com",
    "url": "https://github.com/goncedillo/mockium/issues"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "engines": {
    "node": ">= 0.8.0"
  }
}
